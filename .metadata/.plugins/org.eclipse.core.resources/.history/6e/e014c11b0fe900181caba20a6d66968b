package coreJava.Enums;

public enum TeachingEnum {
	ASSIGN_INSTRUCTOR_TO_COURSE("ALTER teacher"
							  + "INSERT VALUES(?, ?, ?)"),
	
	GET_INSTRUCTORS_TEACHING_COURSES("SELECT * FROM teaching ");
	
	private final String query;
	
	TeachingEnum(String query){
		this.query = query;
	}
}
